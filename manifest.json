{
  "manifest_version": 2,
  "name": "Quick notes",
  "version": "1.0",
  "description": "Ma premi√®re extension chrome",
  "icons": {
    "16": "public/logo/get_started16.png",
    "32": "public/logo/get_started32.png",
    "48": "public/logo/get_started48.png",
    "128": "public/logo/get_started128.png"
  },
  "content_security_policy": "script-src 'self' https://kit.fontawesome.com/; object-src 'self'",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "public/logo/get_started16.png",
      "32": "public/logo/get_started32.png",
      "48": "public/logo/get_started48.png",
      "128": "public/logo/get_started128.png"
    }
  },
  "permissions": ["clipboardWrite"]
}
